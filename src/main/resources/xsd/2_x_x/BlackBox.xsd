<?xml version="1.0" encoding="UTF-8"?>
<xs:schema
        targetNamespace="https://i-t.io/BlackBox/xsd/2_x_x/BlackBox.xsd"
        xmlns="https://i-t.io/BlackBox/xsd/2_x_x/BlackBox.xsd"
        xmlns:xs="http://www.w3.org/2001/XMLSchema"
        elementFormDefault="qualified">
    <xs:annotation>
        <xs:documentation>
            https://github.com/INFINITE-TECHNOLOGY/BLACKBOX/wiki#blackbox-xsd
        </xs:documentation>
    </xs:annotation>
    <!---->
    <xs:include schemaLocation="Argument.xsd"/>
    <xs:include schemaLocation="MethodNode.xsd"/>
    <xs:include schemaLocation="Object.xsd"/>
    <xs:include schemaLocation="Argument.xsd"/>
    <xs:include schemaLocation="Exception.xsd"/>
    <xs:include schemaLocation="ExceptionReference.xsd"/>
    <xs:include schemaLocation="Exception.xsd"/>
    <xs:include schemaLocation="ArgumentList.xsd"/>
    <xs:include schemaLocation="Expression.xsd"/>
    <xs:include schemaLocation="ASTNode.xsd"/>
    <xs:include schemaLocation="ASTNodeList.xsd"/>
    <xs:include schemaLocation="StandaloneException.xsd"/>
    <xs:include schemaLocation="StandaloneExceptionList.xsd"/>
    <xs:include schemaLocation="Statement.xsd"/>
    <!---->
    <xs:element name="rootAstNode" type="ASTNode">
        <xs:annotation>
            <xs:documentation>
                This is the root element in the BlackBox XML log files. Used for non-error (Debug, Trace) log formats.
            </xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="standaloneExceptionList" type="StandaloneExceptionList">
        <xs:annotation>
            <xs:documentation>
                List of standalone exceptions. Used for error log format only.
            </xs:documentation>
        </xs:annotation>
    </xs:element>
</xs:schema>